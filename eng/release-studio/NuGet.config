  <packageSources>
    <clear />
    <!-- Only one feed is allowed. https://aka.ms/cfs/nuget -->
    <add key="MicroBuildToolset" value="https://pkgs.dev.azure.com/dnceng/_packaging/MicroBuildToolset/nuget/v3/index.json" />
