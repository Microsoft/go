-			prog.GOARCH == meta["GOARCH"] {
-			return prog
-		}
-	}
-	prog := telemetry.ProgramReport{
-		Program:   meta["Program"],
-		Version:   meta["Version"],
-		GoVersion: meta["GoVersion"],
-		GOOS:      meta["GOOS"],
-		GOARCH:    meta["GOARCH"],
-		Counters:  make(map[string]int64),
-		Stacks:    make(map[string]int64),
